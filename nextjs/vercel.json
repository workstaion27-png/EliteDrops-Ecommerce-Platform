    "SMTP_PORT": "@smtp_port",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_password",
    "EMAIL_FROM": "@email_from",
    "JWT_SECRET": "@jwt_secret",
    "ENCRYPTION_KEY": "@encryption_key",
    "ADMIN_EMAIL": "@admin_email",
